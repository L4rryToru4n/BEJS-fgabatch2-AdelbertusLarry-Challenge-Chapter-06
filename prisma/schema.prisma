generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id BigInt @id @default(autoincrement())
  name String
  email String @unique
  image_url String?
  image_title String?
  image_description String?
  video_url String?
  video_title String?
  video_description String?
}